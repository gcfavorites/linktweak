{
  "manifest_version": 2,
  "name": "Link Tweak",
  "version": "1.1.2",
  "description": "Rewrite link url (href attribute of A tag) with flexible rules.",
  "icons": { "128": "icon-128.png", "48": "icon-48.png" },
  "background": { "page":"index.html" },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js":      ["Content.js"]
    }
  ]
}
